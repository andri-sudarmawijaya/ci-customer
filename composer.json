{
    "name": "andri-sudarmawijaya/ci-customer",
    "description": "CI Customer Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andri Sudarmawijaya",
            "email": "andri.sudarmawijaya@gmail.com"
        }
    ],
	"require": {
        "slowprog/composer-copy-file": "~0.3"
    },
    "autoload": {
        "psr-4": {
            "AndriSudarmawijaya\\CiCustomer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ],
        "post-update-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "copy-file": {
            "vendor/andri-sudarmawijaya/ci-customer/src/application/": "application/"
        }
    }
}
